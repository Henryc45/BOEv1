<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <%- include('../partials/head'); %>
    <link rel="stylesheet" href="/css/lo.css" />
  </head>
  <body class="lo-body">
    <%- include('../partials/nav'); %>
    <section class="lo-section-main">
      <div class="lo-container lo-container-big">
        <h1>Welcome <%= name %></h1>
        <section class="lo-profile-card lo-profile-card-avatar">
          <p>HELLO</p>
        </section>
        <section class="lo-profile-card-text-side">
          <% if (name) { %>
          <h5><%= name %></h5>
          <% } %> <% if (email) { %>
          <p>
            <a href="mailto:<%=email%>"><%=email%></a>
          </p>
          <% } %> <% if (date) { %>
          <p>Member since <%= date %></p>
          <% } %>
        </section>
        <aside class="lo-dot-menu">
          <i class="fa fa-ellipsis-h" aria-hidden="true"></i>
        </aside>
      </div>
      <div class="lo-container">
        <!-- Favorites List -->
        <%- include('../partials/listings-favorites'); %>
        <aside class="lo-dot-menu">
          <i class="fa fa-ellipsis-h" aria-hidden="true"></i>
        </aside>
      </div>
    </section>
    <%- include('../partials/footer'); %>
  </body>
</html>
